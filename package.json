{
  "name": "n8n-nodes-hudu",
  "version": "1.0.4",
  "description": "This n8n custom node facilitates integration with Hudu's API.",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node-hudu",
    "hudu",
    "hudu-api",
    "knowledgebase-management",
    "n8n-custom-node",
    "workflow-automation",
    "data-operations"
  ],
  "license": "MIT",
  "homepage": "https://github.com/msoukhomlinov/n8n-nodes-hudu",
  "author": {
    "name": "Max Soukhomlinov",
    "email": "maxs@intellectit.com.au"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msoukhomlinov/n8n-nodes-hudu.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "gulp clean && tsc && gulp copy:files && gulp move",
    "dev": "tsc --watch",
    "format": "prettier src nodes credentials --write",
    "lint": "eslint *.ts package.json",
    "lintfix": "eslint *.ts package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js *.ts package.json",
    "test": "jest",
    "clean": "gulp clean"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/HuduApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Hudu/Hudu.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^29.5.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^18.15.11",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "~5.45",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "jest": "^29.5.0",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "luxon": "^3.4.4",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0"
  }
}
